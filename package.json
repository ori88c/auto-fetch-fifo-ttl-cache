    "event-driven",
    "proactive-remove",
    "lru-alternative",
    "uniform-key-distribution",
    "short-term-key-popularity",
    "in-memory",
    "graceful-termination",
    "graceful-teardown",
    "graceful-shutdown",
    "clean-teardown",
    "smooth-shutdown",
    "nodejs",
    "typescript",
    "ts",
    "ES2020"
  ],
  "engines": {
    "node": ">=14.5.0"
  },
  "dependencies": {
    "fifo-ttl-cache": "1.1.0",
    "zero-overhead-keyed-promise-lock": "1.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
